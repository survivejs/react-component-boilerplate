{
  "name": "stupid-rc-starter",
  "description": "Boilerplate for React.js components with storybook",
  "author": "Stupid Sun",
  "user": "stupidism",
  "version": "5.0.0",
  "scripts": {
    "start": "webpack-dev-server --env dev",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint:js": "eslint . --ignore-path .gitignore --ignore-pattern dist --cache",
    "lint:css": "stylelint \"src/**/*.{css,less,scss,sss}\" --allow-empty-input",
    "lint:staged": "lint-staged",
    "lint": "npm run lint:js && npm run lint:css",
    "gh-pages": "npm run gh-pages:readme && npm run gh-pages:storybook",
    "gh-pages:readme": "webpack --env ghPages",
    "gh-pages:storybook": "build-storybook -o gh-pages/storybook",
    "gh-pages:opn": "opn gh-pages/index.html",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --env ghPages --profile --json > stats.json",
    "dist": "webpack --env dist",
    "dist:min": "webpack --env distMin",
    "dist:modules": "rimraf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "preversion": "npm run test && npm run dist && npm run dist:min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js",
    "git:commitable": "git status | grep 'Changes to be committed:'",
    "git:stash-pop": "git stash pop >> /dev/null",
    "git:stash-rebase": "git read-tree stash && git stash drop",
    "pre-commit:stash-unstaged": "git stash save -k --include-untracked 'unstaged-stash' >> /dev/null",
    "pre-commit:lint-staged-or-pop": "(lint-staged && git add .) || (npm run git:stash-pop && exit 1)",
    "pre-commit:commitable-or-pop": "npm run git:commitable || (npm run git:stash-pop && lint-staged && exit 1)",
    "pre-commit:stash-linted": "git stash save 'linted-stash' >> /dev/null",
    "pre-commit:pop-unstaged": "git stash pop stash@{1}",
    "pre-commit:rebase-linted": "npm run git:stash-rebase",
    "pre-commit:clean": "(lint-staged >> /dev/null) || exit 0",
    "storybook": "start-storybook -p 6006 && opn localhost:6006"
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix"
    ],
    "*.{js,jsx}": [
      "eslint --fix"
    ],
    "*.{css,less,scss,sss}": [
      "stylefmt",
      "stylelint"
    ]
  },
  "main": "dist-modules",
  "module": "src",
  "jsnext:main": "src",
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "recompose": "^0.23.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.0.0-alpha.0",
    "@storybook/addon-links": "^3.0.0-alpha.0",
    "@storybook/react": "^3.0.0-alpha.4",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "catalog": "^2.5.3",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.1",
    "editorconfig-tools": "^0.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^0.12.0",
    "git-prepush-hook": "^1.0.2",
    "html-webpack-plugin": "^2.28.0",
    "html-webpack-template": "^6.0.1",
    "jest": "^20.0.0",
    "json-loader": "^0.5.4",
    "lint-staged": "^3.4.2",
    "opn-cli": "^3.1.0",
    "pre-commit": "^1.2.2",
    "purecss": "^0.6.2",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.5.1",
    "react-github-corner": "^0.3.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "stylefmt": "^5.3.2",
    "stylelint": "^7.10.1",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": ">= 0.11.2 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Stupidism/stupid-rc-starter.git"
  },
  "homepage": "https://survivejs.github.io/react-component-boilerplate/",
  "bugs": {
    "url": "https://github.com/Stupidism/stupid-rc-starter/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "starter-kit",
    "storybook"
  ],
  "license": "MIT",
  "pre-commit": [
    "pre-commit:stash-unstaged",
    "pre-commit:lint-staged-or-pop",
    "pre-commit:commitable-or-pop",
    "pre-commit:stash-linted",
    "pre-commit:pop-unstaged",
    "pre-commit:rebase-linted",
    "pre-commit:clean"
  ],
  "pre-push": [
    "test"
  ]
}
